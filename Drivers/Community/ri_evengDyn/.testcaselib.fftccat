        <item name="session_profiles/eve-ng_qc.fftc">
            <tcDescriptor version="25.4.0.202512130118">
                <general>
                    <notes>Quickcall library for EVE-NG</notes>
                    <sessionClass includeTestCase="true" sessionType="project://ri_evengDyn/session_profiles/eve-ng.ffsp"/>
                    <language>Python</language>
                    <xPathVersion>XPATH31</xPathVersion>
                </general>
                <procedures>
                    <item name="evengLogin" isPublic="true">
                        <arguments>
                            <item name="username">
                                <description>eve-ng username</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="password">
                                <description>eve-ng password</description>
                                <isMandatory>true</isMandatory>
                            </item>
                        </arguments>
                    </item>
                    <item name="evengCreateLab" isPublic="true">
                        <arguments>
                            <item name="cookie">
                                <description>eve-ng session cookie</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="lab_name">
                                <description>eve-ng lab name (required)</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="version">
                                <description>eve-ng lab version (required)</description>
                                <defaultValue>1</defaultValue>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="path">
                                <description>eve-ng lab path</description>
                            </item>
                            <item name="author">
                                <description>eve-ng lab author</description>
                            </item>
                            <item name="description">
                                <description>even-ng new lab description</description>
                            </item>
                            <item name="body">
                                <description>eve-ng new lab body</description>
                            </item>
                        </arguments>
                    </item>
                    <item name="evengDeleteLab" isPublic="true">
                        <arguments>
                            <item name="cookie">
                                <description>eve-ng session cookie</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="lab_name">
                                <description>eve-ng lab name (required)</description>
                                <isMandatory>true</isMandatory>
                            </item>
                        </arguments>
                    </item>
                    <item name="evengCreateNode" isPublic="true">
                        <arguments>
                            <item name="cookie">
                                <description>eve-ng cookie</description>
                                <isMandatory>true</isMandatory>
                            </item>
