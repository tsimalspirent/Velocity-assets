                            <item name="cookie">
                                <description>eve-ng cookie</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="lab_name">
                                <description>eve-ng lab name (without .unl)</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="networks_list">
                                <description>eve-ng networks list
e.g. [1, 2]</description>
                            </item>
                            <item name="visibility">
                                <description>eve-ng network id visibility value
e.g. {0, 1}</description>
                            </item>
                        </arguments>
                    </item>
