                                <description>eve-ng node template (includes all required properties)</description>
                                <isMandatory>true</isMandatory>
                            </item>
                        </arguments>
                    </item>
                    <item name="evengStartNode" isPublic="true">
                        <arguments>
                            <item name="cookie">
                                <description>eve-ng cookie</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="lab_name">
                                <description>eve-ng lab name (without .unl)</description>
                                <isMandatory>true</isMandatory>
