                            <item name="lab_name">
                                <description>eve-ng lab name (without .unl)</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="node_id">
                                <description>eve-ng node id</description>
                                <isMandatory>true</isMandatory>
                            </item>
                            <item name="network_id">
                                <description>eve-ng network info
e.g. {&quot;32&quot;:&quot;1&quot;,&quot;21&quot;:&quot;2&quot;} where 32 - is port id and 1 - in network id</description>
                            </item>
